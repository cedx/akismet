{
  "author" : "CÃ©dric Belin <cedric@belin.io>",
  "bugs" : "https://github.com/cedx/akismet.js/issues",
  "description" : "Prevent comment spam using Akismet service.",
  "homepage" : "https://github.com/cedx/akismet.js",
  "license" : "MIT",
  "main" : "./index",
  "name" : "akismet-js",
  "preferGlobal" : false,
  "private" : false,
  "version" : "0.1.0",
  "dependencies" : {
    "request" : "~2.30.0"
  },
  "devDependencies" : {
    "jshint" : "~2.3.0",
    "mocha" : "~1.15.1",
    "yuidocjs" : "~0.3.46"
  },
  "engines" : {
    "node" : "~0.10.23"
  },
  "jshintConfig" : {
    "freeze" : true,
    "indent" : 2,
    "latedef" : true,
    "newcap" : true,
    "node" : true,
    "nonew" : true,
    "quotmark" : "single",
    "strict" : true,
    "trailing" : true,
    "undef" : true,
    "unused" : true
  },
  "keywords" : [
    "akismet",
    "comment",
    "spam",
    "validation"
  ],
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/cedx/akismet.js.git"
  },
  "scripts" : {
    "doc" : "yuidoc --lint && yuidoc --no-code",
    "lint" : "jshint --verbose lib/",
    "test" : "mocha --check-leaks --recursive --reporter spec test/"
  }
}